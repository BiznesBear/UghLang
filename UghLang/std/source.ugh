# Powers X by Y #
fun pow(x, y){
	return(x ** y);
}

# Returns string of fibbonaci sequance separeted with spaces #
fun fib(len){
    r = "";

    a = 0; 
    b = 1;  

    count(len){ 
        c = (a + b); 
        r += (c + " "); 

        a = b;     
        b = c;
    }

    return(r);
}
