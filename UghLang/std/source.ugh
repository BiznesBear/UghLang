# Returns array of fibbonaci sequance #
fun Fib(len){
    r = [len];
    a = 0; 
    b = 1; 
    c = 0;
    index = 0;
    
    repeat len { 
        c = (a + b);
         
        a = b;     
        b = c;
        
        r[index] = c;
        index+1;
    }
    
    return r;
}

